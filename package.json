{
  "name": "luxury-apartments-accra",
  "version": "1.0.0",
  "description": "Luxury brand apartment website for Accra",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "concurrently \"npm run build --prefix client\" \"npm run build --prefix server\"",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": [
    "luxury",
    "apartments",
    "accra",
    "real-estate"
  ],
  "author": "Luxury Apartments Accra",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
